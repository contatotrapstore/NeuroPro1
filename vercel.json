{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 30
    },
    "api/webhook-asaas.js": {
      "maxDuration": 30
    },
    "api/webhook-asaas-test.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/webhooks/asaas",
      "destination": "/api/webhook-asaas"
    },
    {
      "source": "/api/webhooks/asaas-test",
      "destination": "/api/webhook-asaas-test"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With, asaas-signature, x-asaas-signature"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ]
}