{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assistants",
      "dest": "/assistants.js"
    },
    {
      "src": "/health", 
      "dest": "/health.js"
    },
    {
      "src": "/subscriptions",
      "dest": "/subscriptions.js"
    },
    {
      "src": "/test",
      "dest": "/test.js"
    }
  ]
}