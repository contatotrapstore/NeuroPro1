{
  "version": 2,
  "functions": {
    "*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/assistants",
      "destination": "/assistants.js"
    },
    {
      "source": "/health", 
      "destination": "/health.js"
    },
    {
      "source": "/subscriptions",
      "destination": "/subscriptions.js"
    },
    {
      "source": "/test",
      "destination": "/test.js"
    }
  ]
}